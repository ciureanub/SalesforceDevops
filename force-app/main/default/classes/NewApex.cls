public with sharing class NewApex {
    public NewApex() {

    }
    public integer add(integer a, integer b)
    {
        return a+b;
    }

    public integer subtract(integer a, integer b)
    {
        return a-b;
    }

    public integer div(integer a, integer b)
    {
        return a/b;
    } 
    
}
